{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Health Service",
        "Uri": "http://localhost:5000/health/Complete"

      }
    ],
    "EvaluationTimeOnSeconds": 5,//Ui Refresh time
    "MinimumSecondsBetweenFailureNotifications": 120
  }
}
